$cgmx -noplot -noplot dielectricBodies -g=solidSphereRad0p5Gridi4.order2.hdf -innerDomain=innerDomain -tf=100 -tp=.1 -useSosupDissipation=1 -ic=gp -kx=1 -ay=1 -az=0 -x0=-1 -rbc=rbcNonLocal -background=OuterSquare -xLeftProbe=-.9 -xRightProbe=.9 -dm=none -probeFrequency=1 -probeFileName=junkProbe -matFile=baMatIsoEps4.txt -flushFrequency=5 -show=solidSphereRad0p5G4i.show -go=go  >! solidSphereRad0p5G4i.out

------ nrMin=6 : OK 
ogen -noplot solidEllipsoidInABoxGrid -prefix=solidSphereRad0p5Grid -order=2 -per=npp -a=.5 -b=.5 -c=.5 -xa=-1 -xb=1 -nrMin=6 -factor=4

