32 x 32 = 1024 
ogen -noplot diskArrayGrid -prefix=1024DiskGrid -xa=-24.25 -xb=24.25 -ya=-24 -yb=24 -nCylx=32 -nCyly=32 -deltaX=1.5 -deltaY=1.5 -order=2 -periodic=np -factor=2

16x16 = 256 disks 
ogen -noplot diskArrayGrid -prefix=256DiskGrid -xa=-12.25 -xb=12.25 -ya=-12 -yb=12 -nCylx=16 -nCyly=16 -deltaX=1.5 -deltaY=1.5 -order=2 -periodic=np -factor=2
ogen -noplot diskArrayGrid -prefix=256DiskGrid -xa=-12.25 -xb=12.25 -ya=-12 -yb=12 -nCylx=16 -nCyly=16 -deltaX=1.5 -deltaY=1.5 -order=4 -periodic=np -factor=2


8x8 = 64 disks 
