cgmx -noplot multiDomain -g=multiDiskGride4.order2 -dm=gdm -dmFile=gdmMaterial1.txt gdmMaterial2.txt gdmMaterial3.txt gdmMaterial4.txt -backGround=backGround -rbc=rbcNonLocal -kx=.5 -eps0=1 -useSosupDissipation=1 -tf=1. -tp=.5 -ic=gp -x0=-2.5 -beta=120. -show=MD4.show -go=go >! MD4.out


