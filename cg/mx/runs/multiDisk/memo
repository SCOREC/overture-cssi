cgmx -noplot dielectricBodies -g=multiDiskGrid2x2ye2.order2 -dm=none -backGround=backGround -rbc=rbcNonLocal -kx=.5 -eps1=2 -eps2=1. -useSosupDissipation=0 -tf=10. -tp=.05 -ic=gp -x0=-2. -beta=80. -go=og


