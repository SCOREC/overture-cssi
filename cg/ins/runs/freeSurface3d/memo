cgins freeSurface3d -g=freeSurfaceGrid3de.25p.order2.hdf -nu=.05 -tf=2. -tp=.01 -model=ins -surfaceTension=.0 -ad2=1 -ampA=0.0 -ampB=0.5 -ampC=.5 -surfacePredictor="leap-frog" -go=halt

