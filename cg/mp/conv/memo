>>>>>>>>> PLANAR INTERFACE 

########## NON-SQUARES ##############
cgmp -noplot twoDomainsCHT.cmd -g=twoNonSquaresInterfacee1.order2.hdf -dtMax=.05 -useChamp=1 -kappa1=.3 -ktc1=.1 -kappa2=.1 -ktc2=.2 -known=planeInterfaceMode -uMin=-1.5 -uMax=1.2 -tf=.05 -tp=.05 -ts=im -useMultiStage=1 -nc=21 -coupled=0 -itol=1e-12 -debug=3 -go=go > junk2

conv.p twoDomainsCHT.conv -problem=planeInterfaceMode -cg=twoNonSquaresInterface -known=planeInterfaceMode -useChamp=1 -domain1=leftDomain -domain2=rightDomain -tp=.1 -tf=.1 -ng=4

