REVISIT AMP -- turn on SIGMA-JUMP term in velocity correction

***** JUMP:


COMPARE TO DAN


radialWaveConv:
	# radial traveling wave
conv.p insElasticPiston.conv -known=radialTravelingWave -cg=rtw -tp=0.3 -tf=0.3 -showAllComponents=0 -tsINS=im -smVariation=g -caseid=7 -scf=0.1 -nu=0.1  -zfMuByH=1 -amp=1e-7 -addedMass=1 -useImplicitAmpBCs=1 -numberOfCorrections=1 -cfl=0.9 -diss=0.5 -stressRelaxation=4 -startCurve=spline  -setGhostByExtrapolation=1 -zfOption=m -zfMono=1 -ng=3


                grid  &    N &   $p$   & r &     $\vv$    & r &    $\usv$   & r &     $\vsv$   & r &   $\sigmavs$  & r 
                rtw2 &     1 & 5.58e-08 &      & 3.15e-08 &      & 4.27e-08 &      & 3.39e-07 &      & 4.55e-08 &      
                rtw4 &     2 & 8.77e-09 &  6.4 & 9.34e-09 &  3.4 & 6.05e-09 &  7.1 & 6.75e-08 &  5.0 & 8.53e-09 &  5.3 
