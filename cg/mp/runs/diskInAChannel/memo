SCF=50 INCREASE zfMuByH=10 (=5 not big enough)
$cgmp diskInAChannel -method=ins -smVariation=g -g=diskInADeformingChannelGride2.order2 -addedMass=1 -zfMuByH=10 -scf=50 -p0=.1 -fluidSolidCornerFix=0 -tp=.05 -tf=20. -tsINS=im -useImplicitAmpBCs=1 -nc=1 -nu=.05 -pOffset=.0 -diss=.5 -debug=1 -rampedInflow=1 -flushFrequency=4 -multiDomainAlgorithm=1 -cfl=.9 -setGhostByExtrapolation=1 -go=halt 


SCF=100 : G2
$cgmp diskInAChannel -method=ins -smVariation=g -g=diskInADeformingChannelGride2.order2 -addedMass=1 -scf=100 -p0=.1 -fluidSolidCornerFix=0 -tp=.05 -tf=20. -tsINS=im -useImplicitAmpBCs=1 -nc=1 -nu=.05 -pOffset=.0 -diss=.5 -debug=1 -rampedInflow=1 -flushFrequency=4 -multiDomainAlgorithm=1 -cfl=.9 -setGhostByExtrapolation=1 -go=halt


SCF=10 -- OK for some time but then oscillation sets up 
$cgmp diskInAChannel -method=ins -smVariation=g  -g=diskInADeformingChannelGride2.order2  -addedMass=1 -scf=10 -u0=.1 -tp=.01 -tf=20. -tsINS=im -useImplicitAmpBCs=1 -nc=1 -nu=.05 -pOffset=.0  -diss=.5 -debug=1 -rampedInflow=1 -flushFrequency=4 -multiDomainAlgorithm=1 -cfl=.9 -setGhostByExtrapolation=1 -go=halt

SCF=20 -- 
$cgmp diskInAChannel -method=ins -smVariation=g  -g=diskInADeformingChannelGride2.order2  -addedMass=1 -scf=20 -p0=1 -fluidSolidCornerFix=0 -tp=.05 -tf=20. -tsINS=im -useImplicitAmpBCs=1 -nc=1 -nu=.05 -pOffset=.0  -diss=.5 -debug=1 -rampedInflow=1 -flushFrequency=4 -multiDomainAlgorithm=1 -cfl=.9 -setGhostByExtrapolation=1 -go=halt


SCF=50 -- increase p0, show file --> *TROUBLE for later times
$cgmp -noplot diskInAChannel -method=ins -smVariation=g  -g=diskInADeformingChannelGride2.order2  -addedMass=1 -p0=10 -scf=50 -u0=.1 -tp=.1 -tf=2. -tsINS=im -useImplicitAmpBCs=1 -nc=1 -nu=.05 -pOffset=.0  -diss=.5 -debug=1 -rampedInflow=1 -flushFrequency=4 -multiDomainAlgorithm=1 -cfl=.9 -setGhostByExtrapolation=1 -show=diskInADeformingChannelScf50G2.show -go=go >! diskInADeformingChannelScf50G2.out 



SCF=100 -- OK
$cgmp diskInAChannel -method=ins -smVariation=g  -g=diskInADeformingChannelGride2.order2  -addedMass=1 -scf=100 -u0=.1 -tp=.01 -tf=20. -tsINS=im -useImplicitAmpBCs=1 -nc=1 -nu=.05 -pOffset=.0  -diss=.5 -debug=1 -rampedInflow=1 -flushFrequency=4 -multiDomainAlgorithm=1 -cfl=.9 -setGhostByExtrapolation=1 -go=halt

SCF=1000 -- works
$cgmp diskInAChannel -method=ins -smVariation=g  -g=diskInADeformingChannelGride2.order2  -addedMass=1 -scf=1000. -u0=.1 -tp=.01 -tf=20. -tsINS=im -useImplicitAmpBCs=1 -nc=1 -nu=.05 -pOffset=.0  -diss=.5 -debug=1 -rampedInflow=1 -flushFrequency=4 -multiDomainAlgorithm=1 -cfl=.9 -setGhostByExtrapolation=1 -go=halt



